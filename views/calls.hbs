<div class="col-sm-9 col-sm-offset-3 col-md-10 col-md-offset-2 main">
  <h1 class="page-header">Calls list</h1>

  <div class="table-responsive">
    <table class="table table-striped">
      <thead>
        <tr>
          <th>Date Sent</th>
          <th>From</th>
          <th>To</th>
          <th>Body</th>
          <th>Status</th>
        </tr>
      </thead>
      <tbody>
        {{#messages}}
        <tr>
          <td>{{date_sent}}</td>
          <td>{{from}}</td>
          <td>{{to}}</td>
          <td>{{body}}</td> 
          <td>{{status}}</td>        
        </tr>
        {{/messages}}
      </tbody>
    </table>
  </div>

</div>