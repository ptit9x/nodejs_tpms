<div class="col-sm-9 col-sm-offset-3 col-md-10 col-md-offset-2 main">
  <h3 class="page-header">Profile</h3>

  <div class="table-responsive">
    <table class="table table-striped">
      <thead>
        <tr>
          <th>Friendly Name</th>
          <th>Number</th>          
          <th></th>
          <th></th>          
        </tr>
      </thead>
      <tbody>
        {{#each numbers}}       
        <tr>
          <td><a href="/dashboard/number/{{this.phoneNumber}}">{{this.friendlyName}}</a></td>
          <td>{{this.phoneNumber}}</td>        
          <td><a href="/dashboard/messages/{{this.phoneNumber}}">Messages</a></td>
          <td><a href="/dashboard/voices/{{this.phoneNumber}}">Voices</a></td>               
        </tr>
        {{/each}}
      </tbody>
    </table>
  </div>

</div>